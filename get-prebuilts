mkdir -p $PWD/vendor/pax/proprietary

# Get Android Terminal Emulator (we use a prebuilt so it can update from the Market)
curl -L -o $PWD/vendor/pax/proprietary/Term.apk -O -L https://github.com/downloads/jackpal/Android-Terminal-Emulator/Term.apk
unzip -o -d $PWD/vendor/pax/proprietary $PWD/vendor/pax/proprietary/Term.apk lib/armeabi/libjackpal-androidterm4.so

